s":"59","suppressedMessages":"60","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"61","messages":"62","suppressedMessages":"63","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"57"},{"filePath":"64","messages":"65","suppressedMessages":"66","errorCount":0,"fatalErrorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"67","messages":"68","suppressedMessages":"69","errorCount":0,"fatalErrorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/index.js",[],[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/App.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/reportWebVitals.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/directory/directory.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/category-item/category-item.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/routes/home/home.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/routes/navigation/navigation.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/utils/firebase/firebase.utils.js",["70"],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/sign-up-form/sign-up-form.component.js",["71","72"],[],"import { useState } from \"react\";\nimport FormInput from \"../form-input/form-input.component.js\";\nimport Button from \"../button/button.component.js\";\nimport {\n  createAuthUserWithEmailAndPassword,\n  createUserDocumentFromAuth,\n} from \"../../utils/firebase/firebase.utils.js\";\n\nimport \"./sign-up-form.component.js\";\n\n// to avoid writing 4 useState, pass in one object as initial state value in useState\nconst defaultFields = {\n  displayName: \"\",\n  email: \"\",\n  password: \"\",\n  confirmedPassword: \"\",\n};\n\nconst SignUpForm = () => {\n  const [fields, setFields] = useState(defaultFields);\n  const { displayName, email, password, confirmedPassword } = fields;\n\n  console.log(fields);\n\n  const resetFormFields = () => {\n    setFields(defaultFields);\n  };\n\n  // for onSubmit event handler in <form> tag\n  const handleSubmit = async (event) => {\n    // prevent default behavior in the <form>\n    event.preventDefault();\n\n    // check password + confirmed password match\n    if (password !== confirmedPassword) return;\n    // authenticate user in google authentication\n    // why use try catch here??\n    try {\n      // user is a deconstruction of the response.\n      const { user } = await createAuthUserWithEmailAndPassword(\n        email,\n        password\n      );\n      // additional information: displayName if no displayName from userAuth\n      await createUserDocumentFromAuth(user, { displayName });\n      // reset form to empty after the user is created in the function\n      resetFormFields();\n    } catch (error) {\n      if (error.code === \"auth/email-already-in-use\") {\n        alert(\"Cannot create user, email already in use\");\n      } else {\n        console.log(\"user creation encountered an error: \", error);\n      }\n    }\n    // create user in db\n  };\n\n  // generalize handleChange to all form inputs (displayName, email, password, confirmedPassword)\n  const handleChange = (event) => {\n    {\n      /* name is a property in event??????? becuase we have passed 'name' prop in <input> ? is it why? */\n      /* name and value will come in thru event.target object */\n    }\n\n    const { name, value } = event.target;\n    {\n      /* whats the reason to spread out fields like ...fields and then bracket name [name], set it to value that one i get it though */\n      /* we want to spread in the object fields and modify only one value of the object */\n    }\n    setFields({ ...fields, [name]: value });\n  };\n\n  return (\n    <div className=\"sign-up-container\">\n      <h2>Don't have an account?</h2>\n      <span>Sign up with your email and password</span>\n      <form onSubmit={handleSubmit}>\n        {/* name property is needed in input field to generalize handleChange\n        method in onChange event handler */}\n        <FormInput\n          label=\"Display Name\"\n          type=\"text\"\n          required\n          name=\"displayName\"\n          onChange={handleChange}\n          value={displayName}\n        />\n        <FormInput\n          label=\"Email\"\n          type=\"email\"\n          required\n          name=\"email\"\n          onChange={handleChange}\n          value={email}\n        />\n\n        <FormInput\n          label=\"Password\"\n          type=\"password\"\n          required\n          name=\"password\"\n          onChange={handleChange}\n          value={password}\n        />\n\n        <FormInput\n          label=\"Confirm Password\"\n          type=\"password\"\n          required\n          name=\"confirmedPassword\"\n          onChange={handleChange}\n          value={confirmedPassword}\n        />\n        {/* inside <Button if you say buttonType=\"google\" or buttonType=\"inverted\" it changes, otherwise none is default */}\n        <Button type=\"submit\">Sign Up</Button>\n      </form>\n    </div>\n  );\n};\n\nexport default SignUpForm;\n",[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/form-input/form-input.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/button/button.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/routes/authentication/authentication.component.js",[],[],"/Users/viv/Desktop/VS_Code_Projects/crwn-clothing/clothing-shop/src/components/sign-in-form/sign-in-form.component.js",["73"],[],{"ruleId":"74","severity":1,"message":"75","line":20,"column":7,"nodeType":"76","messageId":"77","endLine":20,"endColumn":10},{"ruleId":"78","severity":1,"message":"79","line":60,"column":5,"nodeType":"80","messageId":"81","endLine":63,"endColumn":6},{"ruleId":"78","severity":1,"message":"79","line":66,"column":5,"nodeType":"80","messageId":"81","endLine":69,"endColumn":6},{"ruleId":"74","severity":1,"message":"82","line":5,"column":3,"nodeType":"76","messageId":"77","endLine":5,"endColumn":37},"no-unused-vars","'app' is assigned a value but never used.","Identifier","unusedVar","no-lone-blocks","Nested block is redundant.","BlockStatement","redundantNestedBlock","'createAuthUserWithEmailAndPassword' is defined but never used."]